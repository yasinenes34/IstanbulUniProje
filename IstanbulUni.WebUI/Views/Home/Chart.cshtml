
@{
    ViewBag.Title = "Index";
    Layout = null;
}


@model IEnumerable<IstanbulUni.DAL.Model.WebMaster>
@using System.Collections;
@{
    ArrayList alyear1 = new ArrayList();
    ArrayList alprod1 = new ArrayList();

    foreach (var x in Model)
    {
        alprod1.Add(Convert.ToString(x.createTime.Day));
    }
    for (int i = 0; i < Model.LongCount(); i++)
    {
        for (int j = 0; j < Model.LongCount(); j++)
        {
            if (alprod1[i] == alprod1[j])
            {
                alyear1.Add(i);
            }
        }


    }


    Html.Kendo().Chart()
        .Name("mychart")
        .Title("Domain Eklenme Tarihi")
        .Legend(legend => legend
            .Position(ChartLegendPosition.Bottom))
             .Series(series =>
             {


                 series.Line(alyear1).Name("Domain");


             })
             .ChartArea(area => area
        .Background("")
        .Height(450)

    )

        .CategoryAxis(axis => axis
            .Categories(alprod1)

       )
        .Render();


}
